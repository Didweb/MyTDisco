<IfModule mod_rewrite.c>
    RewriteEngine On

    RewriteBase /MyT/
    Redirect 301 pato/dosx  http://localhost/MyT/pato/dos

    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteRule ^(.*)$ index.php?url=$1 [L]

</IfModule>
